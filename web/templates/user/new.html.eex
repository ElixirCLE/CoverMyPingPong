<h1>Add A New Player</h1>
<%= form_for @changeset, user_path(@conn, :new), fn f -> %>
  <div class="form-group">
    <label class="form-label">Name</label>
    <%= text_input f, :name, class: "form-input input-lg" %>
  </div>
  <div class="form-group">
    <%= submit "Add", class: "btn btn-primary btn-lg" %>
  </div>
<% end %>
